@page
@using Aqt.CoreOracle.[ModuleName]
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model EditModalModel
@{
    Layout = null;
}

<abp-dynamic-form abp-model="[EntityName]" asp-page="/[ModuleName]/[EntityName]s/EditModal">
    <abp-modal>
        <abp-modal-header title="@L["Edit[EntityName]"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="Id" />
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form> 